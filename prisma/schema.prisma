datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model UserPayment {
  order_id       String   @id
  name           String
  email          String
  npm            String
  total          Int
  payment_status String
  createdAt      DateTime @default(now())
  updateAt       DateTime @updatedAt
}
